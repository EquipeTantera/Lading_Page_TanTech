{
  "kind": "collectionType",
  "collectionName": "photo_products",
  "info": {
    "singularName": "photo-product",
    "pluralName": "photo-products",
    "displayName": "photo_product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "photo_id": {
      "type": "uid"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "product_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "photo_product_id"
    }
  }
}
