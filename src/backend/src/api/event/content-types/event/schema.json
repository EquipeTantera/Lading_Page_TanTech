{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "street": {
      "type": "string",
      "required": true
    },
    "postal_code": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "integer",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "start_time": {
      "type": "datetime",
      "required": true
    },
    "end_time": {
      "type": "datetime",
      "required": true
    },
    "note": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "discount": {
      "type": "boolean",
      "required": true
    },
    "photo_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::photo.photo"
    },
    "event_registrarion_card_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-registrarion-card.event-registrarion-card"
    },
    "result_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::result.result",
      "mappedBy": "event_id"
    },
    "partner_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partner.partner",
      "mappedBy": "event_id"
    }
  }
}
